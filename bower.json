{
  "name": "juice",
  "version": "1.0.0",
  "authors": [
    "Constantine Kiriaze <hello@kiriaze.com>"
  ],
  "description": "Sass front end gulp starter kit",
  "main": "gulpfile.js",
  "keywords": [
    "sass",
    "css",
    "bower",
    "gulp",
    "frontend"
  ],
  "license": "GPL",
  "homepage": "https://github.com/kiriaze/juice",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "assets/vendor",
    "test",
    "tests"
  ],
  "overrides": {
    "modernizr": {
      "main": "modernizr.js"
    },
    "simple-jekyll-search": {
      "main": "dest/jekyll-search.js"
    },
    "jquery-easing-original": {
      "main": "jquery.easing.js"
    },
    "foundation-icon-fonts": {
      "main": [
        "*.{eot,svg,ttf,woff}"
      ]
    }
  },
  "dependencies": {
    "jquery": ">= 2.1.0",
    "modernizr": ">= 2.7.2",
    "s-grid": ">=1.0.0",
    "s-animate": ">=1.0.0",
    "simple-jekyll-search": "~1.0.7",
    "jquery-easing-original": "~1.3.2",
    "fastclick": "~1.0.6",
    "simple-anchors": "*",
    "matchHeight": "~0.6.0",
    "midnight.js": "~1.1.0",
    "font-awesome": "~4.3.0",
    "foundation-icon-fonts": "*",
    "wow.js": "~1.1.2",
    "headroom.js": "~0.7.0"
  }
}
